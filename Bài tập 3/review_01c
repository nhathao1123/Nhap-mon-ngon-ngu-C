#include <stdio.h>

int main(int argc, char *argv[]) 
{
	int n ;
	printf ("nhap so n =") ;
	scanf ("%d",&n)	;
	if (n==1)
	printf ("ban nhap vao onefor1");
	if (n==2)
	printf ("ban nhap vaof onefor2");
	if (n==3)
	printf ("ban nhap vaof onefor3");
	if (n==4)
	printf ("ban nhap vao onefor4");
	if (n==5)
	printf ("ban nhap vao onefor5");
	if (n==6)
	printf ("ban nhap vao onefor6");
	if (n==7)
	printf ("ban nhap vao onefor7");
	if (n==8)
	printf ("ban nhap vao onefor8");
	if (n==9)
	printf ("ban nhap vao onefor9");
	if (n>9)
	printf ("ban nhap vao greate than 9");
	if (n<1)
	printf ("ban nhap vao greate than 9")
	
	return 0;
}
